  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc -p . && npm run copy-views",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/data-source.ts",
    "migration:create": "npm run typeorm -- migration:create -d src/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/data-source.ts",
    "deploy": "npm install && npm run build && npm run typeorm -- migration:run -d dist/data-source.js",
    "copy-views": "cp -R src/views dist/views",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
